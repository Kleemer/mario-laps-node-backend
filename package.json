{
  "name": "mario-laps-node-backend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "tsc-watch --skipLibCheck --outDir dist --onSuccess 'nodemon -w openapi.yml dist/index' | bunyan -o short ",
    "build": "tsc",
    "test": "yarn test:unit src/**/*.spec.ts && yarn test:integration",
    "test:unit": "DOTENV=.env.test jest",
    "test:integration": "DOTENV=.env.test jest test/*.spec.ts",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "aws-sdk": "2.858.0",
    "axios": "0.21.1",
    "axios-oauth-client": "1.4.0",
    "body-parser": "1.19.0",
    "bunyan": "1.8.15",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-openapi-validator": "4.12.4",
    "jsonwebtoken": "8.5.1",
    "mysql": "2.18.1",
    "p-limit": "3.1.0",
    "reflect-metadata": "0.1.13",
    "routing-controllers": "0.9.0",
    "typeorm": "0.2.31",
    "uuid": "8.3.2",
    "yamljs": "0.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/bunyan": "1.8.6",
    "@types/faker": "5.1.7",
    "@types/jest": "26.0.20",
    "@types/jsonwebtoken": "8.5.0",
    "@types/multer": "1.4.5",
    "@types/node": "14.14.32",
    "@types/supertest": "2.0.10",
    "@types/yamljs": "0.2.31",
    "faker": "5.4.0",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "prettier": "2.2.1",
    "supertest": "6.1.3",
    "ts-jest": "26.5.3",
    "ts-node": "9.1.1",
    "tsc-watch": "4.2.9",
    "typescript": "4.2.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testRegex": "(src|test).*.spec.ts$"
  }
}
